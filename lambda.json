{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowReadFromCloudTrailBucket",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject"
      ],
      "Resource": "arn:aws:s3:::google-feed-secops-bucket/*"
    },
    {
      "Sid": "AllowSendMessageToDestinationQueue",
      "Effect": "Allow",
      "Action": [
        "sqs:SendMessage"
      ],
      "Resource": "arn:aws:sqs:us-east-1:841162707314:Destination-Test-Queue"
    },
    {
      "Sid": "AllowCloudWatchLogging",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "*"
    }
  ]
}
