{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "Deny non-HTTPS access",
            "Effect": "Deny",
            "Principal": {
                "Service": "guardduty.amazonaws.com"
            },
            "Action": "s3:*",
            "Resource": "arn:aws:s3:::782439390712-googlesecops-aws-guardduty-logs/*",
            "Condition": {
                "Bool": {
                    "aws:SecureTransport": "false"
                }
            }
        },
        {
            "Sid": "Deny incorrect encryption header",
            "Effect": "Deny",
            "Principal": {
                "Service": "guardduty.amazonaws.com"
            },
            "Action": "s3:PutObject",
            "Resource": "arn:aws:s3:::782439390712-googlesecops-aws-guardduty-logs/*",
            "Condition": {
                "StringNotEquals": {
                    "s3:x-amz-server-side-encryption-aws-kms-key-id": "arn:aws:kms:us-east-2:782439390712:key/9f60c43e-ab49-48d2-ab6c-55808540043a"
                }
            }
        },
