{
  "Version": "2012-10-17",
  "Id": "PolicyForSNS",
  "Statement": [
    {
      "Sid": "AllowSNS",
      "Effect": "Allow",
      "Principal": {
        "Service": "sns.amazonaws.com"
      },
      "Action": "SQS:SendMessage",
      "Resource": "arn:aws:sqs:us-east-1:841162707314:CloudTrail-Notification-Queue",
      "Condition": {
        "ArnEquals": {
          "aws:SourceArn": "arn:aws:sns:us-east-1:841162707314:CloudTrail-Notification-Topic"
        }
      }
    }
  ]
}
